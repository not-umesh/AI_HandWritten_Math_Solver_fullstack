{
  "expo": {
    "name": "AI Math Solver",
    "slug": "ai-math-solver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.notumesh.aimathsolver"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a2e"
      },
      "package": "com.notumesh.aimathsolver",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow AI Math Solver to access camera to scan equations."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow AI Math Solver to access photos."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c0e75f0b-1f56-4ffd-8df4-60c8a87a4b69"
      }
    },
    "owner": "notsickboy"
  }
}
